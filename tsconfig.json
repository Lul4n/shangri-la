{
    "compilerOptions": {
      "target": "ES2022",
      "module": "commonjs",
      "outDir": "out",
      "sourceMap": true,
      "rootDir": "src",
      "incremental": true, 
      
      "strict": true,
      "noImplicitAny": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
    }
}